             type="font"
             default="normal normal 350% 'Norican', cursive;" value="normal normal 36px Permanent Marker">
   <Variable name="descriptionfont" description="Blog Description Font"
             type="font"
             default="normal normal 90% 'Norican', Cursive;" value="normal normal 90% 'Norican', Cursive;">
